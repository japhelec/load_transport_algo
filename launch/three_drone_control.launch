<?xml version="1.0"?>
<launch>	
	<node pkg="load_transport" name="control" type="main_control_three.py" output="screen">
		<param name="tello1_ns" value="$(arg tello1_ns)" />
		<param name="tello2_ns" value="$(arg tello2_ns)" />
		<param name="tello3_ns" value="$(arg tello3_ns)" />
		<param name="flyup_kpx" value="$(arg flyup_kpx)" />
		<param name="flyup_kpy" value="$(arg flyup_kpy)" />
		<param name="flyup_kpz" value="$(arg flyup_kpz)" />
		<param name="flyup_kix" value="$(arg flyup_kix)" />
		<param name="flyup_kiy" value="$(arg flyup_kiy)" />
		<param name="flyup_kiz" value="$(arg flyup_kiz)" />
		<param name="flyup_kdx" value="$(arg flyup_kdx)" />
		<param name="flyup_kdy" value="$(arg flyup_kdy)" />
		<param name="flyup_kdz" value="$(arg flyup_kdz)" />
		<param name="flyup_tol" value="$(arg flyup_tol)" />
		<param name="lift_thrust_01" value="$(arg lift_thrust_01)" />
		<param name="lift_thrust_02" value="$(arg lift_thrust_02)" />
		<param name="lift_thrust_03" value="$(arg lift_thrust_03)" />
		<param name="lift_duration" value="$(arg lift_duration)" />
		<param name="height_kpz" value="$(arg height_kpz)" />
		<param name="height_d" value="$(arg height_d)" />
	</node>

	<node pkg="rosbag" type="record" name="recorder" output="screen" if="$(arg is_record)" args="
	record -o $(arg dir_ns)/$(arg bag_name).bag 
	/state_transition 
	/$(arg tello1_ns)/cmd_vel 
	/$(arg tello1_ns)/land 
	/$(arg tello1_ns)/manual_takeoff 
	/$(arg tello1_ns)/camera/camera_info 
	/$(arg tello1_ns)/camera/compressed/compressed 
	/$(arg tello1_ns)/imu 
	/$(arg tello1_ns)/odom 
	/$(arg tello1_ns)/status 
	/$(arg tello1_ns)/Mc/raw 
	/$(arg tello1_ns)/Mc/filtered 
	/$(arg tello1_ns)/Ql/raw 
	/$(arg tello1_ns)/Ql/error 
	/$(arg tello1_ns)/Ql/filtered  
	/$(arg tello1_ns)/cRm/raw 
	/$(arg tello1_ns)/cRm/filtered 
	/$(arg tello1_ns)/height/raw 
	/$(arg tello1_ns)/height/filtered 
	/$(arg tello2_ns)/cmd_vel 
	/$(arg tello2_ns)/land 
	/$(arg tello2_ns)/manual_takeoff 
	/$(arg tello2_ns)/camera/camera_info 
	/$(arg tello2_ns)/camera/compressed/compressed 
	/$(arg tello2_ns)/imu 
	/$(arg tello2_ns)/odom 
	/$(arg tello2_ns)/status 
	/$(arg tello2_ns)/Mc/raw 
	/$(arg tello2_ns)/Mc/filtered 
	/$(arg tello2_ns)/Ql/raw 
	/$(arg tello2_ns)/Ql/error 
	/$(arg tello2_ns)/Ql/filtered  
	/$(arg tello2_ns)/cRm/raw 
	/$(arg tello2_ns)/cRm/filtered 
	/$(arg tello2_ns)/height/raw 
	/$(arg tello2_ns)/height/filtered 
	/$(arg tello3_ns)/cmd_vel 
	/$(arg tello3_ns)/land 
	/$(arg tello3_ns)/manual_takeoff 
	/$(arg tello3_ns)/camera/camera_info 
	/$(arg tello3_ns)/camera/compressed/compressed 
	/$(arg tello3_ns)/imu 
	/$(arg tello3_ns)/odom 
	/$(arg tello3_ns)/status 
	/$(arg tello3_ns)/Mc/raw 
	/$(arg tello3_ns)/Mc/filtered 
	/$(arg tello3_ns)/Ql/raw 
	/$(arg tello3_ns)/Ql/error 
	/$(arg tello3_ns)/Ql/filtered  
	/$(arg tello3_ns)/cRm/raw 
	/$(arg tello3_ns)/cRm/filtered 
	/$(arg tello3_ns)/height/raw 
	/$(arg tello3_ns)/height/filtered
	"/>
</launch>
